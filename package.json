{
  "name": "@opendata-mvcr/assembly-line-shared",
  "version": "0.1.0",
  "description": "Common front end components for Assembly Line",
  "homepage": "https://github.com/opendata-mvcr/assembly-line-shared#readme",
  "author": "Karel Kl√≠ma",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "ttsc --project tsconfig.prod.json",
    "watch": "ttsc --project tsconfig.prod.json --watch",
    "format": "prettier --write \"./**/*.{ts,tsx,json}\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opendata-mvcr/assembly-line-shared.git"
  },
  "bugs": {
    "url": "https://github.com/opendata-mvcr/assembly-line-shared/issues"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.3"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/test/**/*.ts?(x)"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ]
  },
  "dependencies": {
    "yaml": "^1.10.2"
  }
}
